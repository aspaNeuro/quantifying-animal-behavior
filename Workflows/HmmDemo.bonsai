            <Expression xsi:type="IncludeWorkflow" Path="Bonsai.ML.LinearDynamicalSystems:Kinematics.CreateKFModel.bonsai">
              <Name>model</Name>
              <Fps>50</Fps>
              <Pos_x0>640.20025634765625</Pos_x0>
              <Pos_y0>462.7911376953125</Pos_y0>
              <Vel_x0>0</Vel_x0>
              <Vel_y0>0</Vel_y0>
              <Acc_x0>0</Acc_x0>
              <Acc_y0>0</Acc_y0>
              <Sigma_a>10000</Sigma_a>
              <Sigma_x>100</Sigma_x>
              <Sigma_y>100</Sigma_y>
              <Sqrt_diag_V0_value>0.001</Sqrt_diag_V0_value>
            </Expression>
            <Expression xsi:type="rx:BehaviorSubject">
              <Name>LdsModelParameters</Name>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="p4:StickyTransitions">
                <p4:Alpha>1</p4:Alpha>
                <p4:Kappa>1000000</p4:Kappa>
              </Combinator>
            </Expression>
            <Expression xsi:type="PropertyMapping">
              <PropertyMappings>
                <Property Name="Transitions" />
              </PropertyMappings>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="p3:StateParameters" />
            </Expression>
            <Expression xsi:type="PropertyMapping">
              <PropertyMappings>
                <Property Name="StateParameters" />
              </PropertyMappings>
            </Expression>
            <Expression xsi:type="IncludeWorkflow" Path="Bonsai.ML.HiddenMarkovModels:CreateHMM.bonsai">
              <Name>hmm</Name>
              <NumStates>6</NumStates>
              <Dimensions>2</Dimensions>
              <ObservationsModelType>Gaussian</ObservationsModelType>
              <TransitionsModelType>Sticky</TransitionsModelType>
            </Expression>
            <Expression xsi:type="rx:BehaviorSubject">
              <Name>HmmModelParameters</Name>
            </Expression>
