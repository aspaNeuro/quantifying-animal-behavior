<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>PackageExample</Title>
    <Description>A package for the Bonsai visual programming language.</Description>
    <Authors>aspaNeuro, NeuroThom</Authors>
    <Copyright>Copyright © aspaNeuro, NeuroThom 2024</Copyright>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">true</GeneratePackageOnBuild>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageIcon></PackageIcon>
    <PackageOutputPath>$(SolutionDir)bin</PackageOutputPath>
    <PackageTags>Bonsai Rx PackageExample</PackageTags>
    <TargetFramework>net472</TargetFramework>
    <Features>strict</Features>
    <Version>0.1.0</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bonsai.Core" Version="2.8.2" />
  </ItemGroup>

</Project>
